{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start:docker": "npm run dev-server:docker",
    "start:local": "npm run dev-server:local",
    "init-db:docker": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "init-db:local": "dotenv -e .env.development.local -- npx prisma migrate dev name --init",
    "prepare-prisma-dev_deprecated": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "dev-server:docker": "nodemon -L -r dotenv/config --exec ts-node-transpile-only src/index.ts dotenv_config_path=.env.development",
    "dev-server:local": "nodemon -L -r dotenv/config --exec ts-node-transpile-only src/index.ts dotenv_config_path=.env.development.local",
    "test": "dotenv -e .env.test -- ts-mocha ./src/__tests__/**/*.ts",
    "prepare-prisma-test": "dotenv -e .env.test -- npx prisma migrate dev",
    "test-server": "npm run prepare-prisma-test && dotenv -e .env.test -- nodemon --watch ./**/*.ts --exec ts-node --transpile-only src/index.ts"
  },
  "prisma": {
    "seed": "dotenv -e .env.development -- ts-node --transpile-only ./prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "@types/chai": "^4.3.5",
    "@types/cls-hooked": "^4.3.4",
    "@types/cookie-parser": "^1.4.4",
    "@types/uuid": "^9.0.4",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "cls-hooked": "^4.2.2",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "mocha": "^10.2.0",
    "redis": "^4.6.10",
    "sinon": "^15.2.0",
    "socket.io": "^4.7.1",
    "supertest": "^6.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.9.0",
    "zod": "3.21.1",
    "zod-prisma-types": "^2.7.4",
    "zod-validation-error": "^1.3.1"
  },
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.15",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.16.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
